{
  "name": "clownface-shacl-path",
  "description": "Find nodes in graph by following SHACL Paths",
  "version": "1.1.0",
  "main": "index.js",
  "type": "module",
  "files": [
    "**/*.js",
    "**/*.d.ts"
  ],
  "scripts": {
    "prepare": "husky install",
    "prepack": "npm run build",
    "build": "run-p build:*",
    "build:cjs": "tsc",
    "build:esm": "BABEL_ENV=modules babel src --out-dir . --extensions .ts --out-file-extension .mjs",
    "test": "c8 --all --reporter=lcov mocha --recursive 'test/*.test.ts'",
    "release": "changeset publish"
  },
  "dependencies": {
    "@changesets/cli": "^2.16.0",
    "@rdf-esm/term-set": "^0.5.0",
    "@tpluscode/rdf-ns-builders": "^1.0.0",
    "@tpluscode/rdf-string": "^0.2.24"
  },
  "peerDependencies": {
    "clownface": "^1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@rdf-esm/data-model": "^0.5.4",
    "@rdf-esm/dataset": "^0.5.1",
    "@rdf-esm/namespace": "^0.5.3",
    "@tpluscode/eslint-config": "^0.3.0",
    "@types/chai": "^4.2.19",
    "@types/clownface": "^1.2.0",
    "@types/mocha": "^8.2.2",
    "@types/rdf-js": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-plugin-add-import-extension": "^1.5.1",
    "c8": "^7.1.2",
    "chai": "^4.3.4",
    "clownface": "^1.3.0",
    "eslint": "^7.30.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^9.0.2",
    "standard": "^16.0.3",
    "typescript": "^3.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tpluscode/clownface-shacl-path"
  },
  "author": "Tomasz Pluskiewicz <awesome@hypermedia.app>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "mocha": {
    "watch-files": [
      "./**/*.ts"
    ],
    "require": [
      "test-setup.cjs"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix --quiet"
    ]
  }
}
